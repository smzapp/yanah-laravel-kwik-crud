{
    "name": "yanah/laravel-kwik-crud",
    "description": "A Laravel package designed to streamline and accelerate development. Let's make it 'kwik' (quick)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Amador",
            "email": "leumasrodama2009@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Yanah\\LaravelKwik\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yanah\\LaravelKwik\\KwikServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
